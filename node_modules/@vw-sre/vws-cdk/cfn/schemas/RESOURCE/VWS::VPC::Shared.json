  "sourceUrl": "https://github.com/vwvws/vws-cfn-vpc-shared-resource/",
  "definitions": {},
  "properties": {
    "Id": {
      "description": "unique vpc share identifier",
      "type": "string"
    },
    "VpcId": {
      "description": "id of the shared vpc",
      "type": "string"
    },
    "Subnets": {
      "description": "list of all available subnets",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false,
  "required": [],
  "readOnlyProperties": [
    "/properties/Id",
    "/properties/VpcId",
    "/properties/Subnets"
  ],
  "primaryIdentifier": [
    "/properties/Id"
  ],
  "handlers": {
    "create": {
      "permissions": [
        "execute-api:Invoke"
      ]
    },
    "read": {
      "permissions": [
        "execute-api:Invoke"
      ]
    },
    "delete": {
      "permissions": [
        "execute-api:Invoke"
      ]
    }
  }
}
