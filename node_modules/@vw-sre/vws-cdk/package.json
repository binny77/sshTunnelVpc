{
  "name": "@vw-sre/vws-cdk",
  "version": "2.6.0",
  "description": "Provide constructs of VWS 2.0 custom resources",
  "keywords": [
    "vws"
  ],
  "author": "SRE-Team <sre-support@vwsdc.io>",
  "homepage": "https://github.com/vw-sre/vws-cdk#readme",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "find lib test \\( -name \"*.js\" -o -name \"*.d.ts\" \\) -type f -delete && rm -f tsconfig.tsbuildinfo",
    "prebuild": "npm run clean && ./generate-l1.sh && git --no-pager diff --exit-code -- lib/generated/",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint lib/index.ts 'lib/**/!(*.d).ts' 'test/**/!(*.d).ts' --fix",
    "package": "npm pack"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vw-sre/vws-cdk.git"
  },
  "bugs": {
    "url": "https://github.com/vw-sre/vws-cdk/issues"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.33.0",
    "constructs": "^10.1.56"
  },
  "devDependencies": {
    "@aws-cdk/assert": "2.41.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "18.7.18",
    "aws-cdk": "^2.42.0",
    "aws-cdk-lib": "2.42.0",
    "cdk-import": "file:cdk-import-0.0.0.tgz",
    "constructs": "10.1.106",
    "eslint": "^8.23.1",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
